{
  "name": "latercart",
  "version": "1.0",
  "description": "latercart chrome extension",
  "manifest_version": 2,
  "permissions": ["activeTab", "declarativeContent", "storage", "http://localhost:3000/"],
  "icons": {
  },
  "background": {
    "scripts": ["script.js"]    },
  "page_action": {"default_popup": "popup.html"},
  "content_security_policy": "script-src 'self' https://unpkg.com/axios/dist/axios.min.js; object-src 'self'"
}